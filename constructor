#Constructor

class student:
    roll = ""
    gpa = ""
    def __init__(self,roll,gpa):
     self.roll = roll
     self.gpa = gpa
    def display(self):
        print(f"Roll : {self.roll},GPA : {self.gpa}")     
Muslim = student(1011,3.85)
Muslim.display()
 
Khadija = student(1011,3.85)
Khadija.display()
